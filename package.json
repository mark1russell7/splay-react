  "dependencies": {
    "@mark1russell7/splay": "github:mark1russell7/splay#main"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/react": "^19.0.0",
    "@mark1russell7/cue": "github:mark1russell7/cue#main"
  },
